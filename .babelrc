{
  "presets": [
    ["@babel/preset-env", {
      "modules": false
    }]
  ],
  "env": {
    "test": {
      "presets": ["@babel/preset-env"],
      "plugins": ["dynamic-import-node", "@babel/plugin-syntax-dynamic-import"]
    },
    "esm": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": false,
            "targets": {
              "esmodules": true
            }
          }
        ]
      ]
    }
  }
}
